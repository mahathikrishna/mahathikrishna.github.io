// Color system

$white: #fff;
$gray-100: #f1f0f1;
$gray-200: #e3e2e3;
$gray-300: #d4d4d4;
$gray-400: #c6c5c6;
$gray-500: #a3a2a3;
$gray-600: #7f7f7f;
$gray-700: #646464;
$gray-800: #494748;
$gray-900: #2e2c2d;
$black: #000;

$grays: ();
// stylelint-disable-next-line scss/dollar-variable-default
$grays: map-merge(("100": $gray-100,
    "200": $gray-200,
    "300": $gray-300,
    "400": $gray-400,
    "500": $gray-500,
    "600": $gray-600,
    "700": $gray-700,
    "800": $gray-800,
    "900": $gray-900),
  $grays);

$blue: #0066b2;
$indigo: #5523CC;
$purple: #ab4a9c;
$pink: #CC00A0;
$red: #da291c;
$orange: #FF9933;
$yellow: #FFC800;
$green: #9fcc00;
$teal: #8fcef1;
$cyan: #10b0e6;